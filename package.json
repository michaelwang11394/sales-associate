    "@shopify/app": "3.49.3",
    "@shopify/app-bridge-types": "^0.0.3",
    "@shopify/cli": "3.49.3",
    "@shopify/polaris": "^11.1.2",
    "@shopify/shopify-app-remix": "^1.3.0",
    "@shopify/shopify-app-session-storage-prisma": "^1.1.1",
    "@shopify/web-pixels-extension": "^0.4.20",
    "@supabase/supabase-js": "^2.37.0",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "bundler": "^0.8.0",
    "clsx": "^2.0.0",
    "cross-env": "^7.0.3",
    "isbot": "latest",
    "langchain": "^0.0.157",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "npm-run-all": "^4.1.5",
    "openai": "^4.10.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prisma": "^4.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ruby": "^0.6.1",
    "sort-by": "^1.2.0",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.9",
    "vite-plugin-shopify": "^2.2.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.1",
    "@remix-run/eslint-config": "^1.19.1",
    "@types/eslint": "^8.40.0",

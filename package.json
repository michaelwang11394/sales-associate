{
  "name": "salesagent",
  "private": true,
  "scripts": {
    "dev": "run-p -sr vite:dev shopify:dev",
    "dev:vercel": "VITE_VERCEL_LOCATION=http://localhost:3000 run-p -sr vite:dev shopify:dev",
    "build": "run-s vite:build shopify:build",
    "deploy:extension": "run-s vite:build cleanup shopify:deploy",
    "shopify": "shopify",
    "shopify:build": "shopify app build",
    "shopify:dev": "shopify app dev",
    "shopify:info": "shopify app info",
    "shopify:generate": "shopify app generate",
    "shopify:deploy": "shopify app deploy",
    "vite:setup": "mkdir -p extensions/chat-interface/snippets/ && touch extensions/chat-interface/snippets/vite-generator.liquid",
    "vite:dev": "run-s vite:setup && vite",
    "vite:build": "run-s vite:setup && vite build",
    "cleanup": "rimraf --glob extensions/*/assets/*.{json,geetkeep}",
    "remix-build": "NODE_ENV=production remix build",
    "deploy:admin": "vercel deploy",
    "predev": "prisma generate && prisma migrate deploy",
    "start": "remix-serve build",
    "docker-start": "npm run setup && npm run start",
    "setup": "prisma generate && prisma migrate deploy",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "prisma": "prisma",
    "dev:vercel-api": "cd api && VERCEL_HOST=localhost:3000 vercel dev",
    "deploy:vercel-api": "cd api && vercel deploy",
    "dev:run-full-stack": "run-p -rs dev:vercel-api dev:vercel"
  },
  "dependencies": {
    "@remix-run/node": "^1.19.1",
    "@remix-run/react": "^1.19.1",
    "@remix-run/serve": "^1.19.1",
    "@shopify/app": "3.49.3",
    "@shopify/app-bridge-types": "^0.0.3",
    "@shopify/cli": "3.49.3",
    "@shopify/polaris": "^12.6.0",
    "@shopify/shopify-app-remix": "^1.3.0",
    "@shopify/shopify-app-session-storage-prisma": "^2.0.2",
    "@shopify/web-pixels-extension": "^0.4.20",
    "@supabase/supabase-js": "^2.37.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "best-effort-json-parser": "^1.0.1",
    "bundler": "^0.8.0",
    "clsx": "^2.0.0",
    "cross-env": "^7.0.3",
    "isbot": "latest",
    "langchain": "^0.0.203",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.1",
    "npm-run-all": "^4.1.5",
    "openai": "^4.10.0",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "replicate": "^0.18.1",
    "ruby": "^0.6.1",
    "sort-by": "^1.2.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.3.2",
    "vite": "^5.0.10",
    "vite-plugin-css-injected-by-js": "^3.3.1",
    "vite-plugin-shopify": "^3.0.0",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.1",
    "@remix-run/eslint-config": "^1.19.1",
    "@types/eslint": "^8.40.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8"
  },
  "workspaces": [
    "frontend",
    "extensions/*"
  ],
  "author": "michaelwang"
}
